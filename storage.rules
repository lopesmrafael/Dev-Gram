rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Permite upload e download de fotos para usu√°rios autenticados
    match /fotos/{imageId} {
      allow read, write: if request.auth != null;
    }
    
    // Regra geral para outros arquivos
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}